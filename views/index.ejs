<!-- É HTML, só troca a extensão -->
<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Guia perguntas</title>
</head>
<body>
    <h1>Guia perguntas!</h1>
    <hr>
    <h3>Lista de perguntas</h3>

    <!--  <%= %>  Exibir o valor da variável -->

    <p><%= nome %> </p>
    <%= lang %>
    <p>Empresa: <%= empresa %></p>

    <!-- Condição true, false -->    
    <% if (msg){ %>
        <h3>ERROR</h3>
    <%} else {%>
        <h3>Nenhum erro</h3>
    <%}%>

    <% produtos.forEach(function(produto){ %>
        <hr>
        <%= produto.nome %>
        <%= produto.preco %>
        <br>
    <% }) %>
</body>
</html>